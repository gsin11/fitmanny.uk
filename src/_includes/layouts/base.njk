<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">

  <link href="{{ '/assets/css/bootstrap.min.css' | url }}" rel="stylesheet" type="text/css">
  <link href="{{ '/assets/css/font-awesome.min.css' | url }}" rel="stylesheet" type="text/css">
  <link href="{{ '/assets/css/owl.carousel.min.css' | url }}" rel="stylesheet" type="text/css">
  <link href="{{ '/assets/css/hover.css' | url }}" rel="stylesheet" type="text/css">
  <link href="{{ '/assets/css/jquery.fancybox.css' | url }}" rel="stylesheet" type="text/css">
  <link href="{{ '/assets/css/swiper.min.css' | url }}" rel="stylesheet" type="text/css">
  <link href="{{ '/assets/css/main.css' | url }}" rel="stylesheet" type="text/css">
  <link href="{{ '/assets/css/responsive.css' | url }}" rel="stylesheet" type="text/css">
  
  <!-- capture the CSS content as a Nunjucks variable -->
  {% set css %}
    {% include "css/critical.css" %}
  {% endset %}
  <!-- feed it through our cssmin filter to minify -->
  <style>
    {{ css | cssmin | safe }}
  </style>

  <link rel="icon" href="{{ '/assets/icons/favicon-32x32.png' | url }}" type="image/png">
  <meta name="theme-color" content="#9e9e9e">
  <link rel="apple-touch-icon" sizes="48x48" href="/assets/icons/favicon-48x48.png">
  <link rel="apple-touch-icon" sizes="72x72" href="/assets/icons/favicon-72x72.png">
  <link rel="apple-touch-icon" sizes="96x96" href="/assets/icons/favicon-96x96.png">
  <link rel="apple-touch-icon" sizes="144x144" href="/assets/icons/favicon-144x144.png">
  <link rel="apple-touch-icon" sizes="192x192" href="/assets/icons/favicon-192x192.png">
  <link rel="apple-touch-icon" sizes="256x256" href="/assets/icons/favicon-256x256.png">
  <link rel="apple-touch-icon" sizes="384x384" href="/assets/icons/favicon-384x384.png">
  <link rel="apple-touch-icon" sizes="512x512" href="/assets/icons/favicon-512x512.png">

  <title>{{ title }}</title>

  <meta property="og:url" content="{{userData.websiteUrl}}">
  <meta property="og:author" content="{{userData.siteName}}">
  <meta property="og:site_name" content="{{userData.siteName}} - {{userData.subtitle}}">
  <meta name="google-site-verification" content="{{userData.googleSiteVerificationCode}}">

  <meta name="description" content="{{userData.ogDescription}}">
  <meta property="og:description" content="{{userData.ogDescription}}">
  <meta property="og:type" content="website">
  <meta property="og:image" content="{{userData.websiteUrl}}">
  <meta name="twitter:description" content="{{userData.ogDescription}}">
  <meta name="twitter:image" content="{{userData.websiteUrl}}">
  <meta property="og:title" content="{{userData.siteName}}">
</head>
<body class="home-page">
  <!-- .loader -->
  {% include "partials/_loader.html" %}
  <!-- /.loader -->

  <!-- header -->
  {% include 'partials/_header.html' %}
  <!-- // header -->
  <main>
    {{ content | safe }}
  </main>
  {% include "partials/_footer.html" %}

  <!-- JS Librarys -->
  <!-- JS libs  -->
  <script src="https://code.jquery.com/jquery-1.11.0.min.js" crossorigin="anonymous"></script>

  <script src="{{ '/assets/js/bootstrap.bundle.min.js' | url }}" type="text/javascript"></script>

  <!-- Owl carousel JS  -->
  <script src="{{ '/assets/js/owl.carousel.js' | url }}" type="text/javascript"></script>

  <!-- Fancybox JS -->
  <script src="{{ '/assets/js/jquery.fancybox.js' | url }}" type="text/javascript"></script>

  <!-- Swiper JS -->
  <script src="{{ '/assets/js/swiper.min.js' | url }}" type="text/javascript"></script>

  <!-- Custome JS -->
  <script src="{{ '/assets/js/main.js' | url }}" type="text/javascript"></script>

  <!-- capture the JS content as a Nunjucks variable -->
  {% set js %}
    {% include "js/scripts.js" %}
  {% endset %}
  <!-- feed it through our jsmin filter to minify -->
  <script>
    {{ js | jsmin | safe }}
  </script>
</body>
</html>
